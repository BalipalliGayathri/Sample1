<?xml version="1.0"?>
<project name="Git" default="CloneApplication">
	<target name="CloneApplication">
	
	   <exec dir="." executable="cmd" >
			<arg line="/c git add ."/>
		</exec>
		 <exec dir="." executable="cmd" >
			<arg line="/c git commit -m 'changes'"/>
		</exec>
		
		<exec dir="." executable="cmd" >
			<arg line="/c git push origin master"/>
		</exec>	
	</target>
</project>
